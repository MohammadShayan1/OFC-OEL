const int irPin = 18; // GPIO to IR LED
const size_t audio_len = sizeof(audio) / sizeof(audio[0]);
  // Define IR LED pin

// PAM modulation parameters
const unsigned int carrierFreq = 38000;  // 38kHz carrier frequency
const unsigned int maxDutyCycle = 100;   // Maximum duty cycle (%)
const unsigned int minDutyCycle = 10;    // Minimum duty cycle (%)
const unsigned long sampleDelay = 125;   // Sample timing (125Âµs = 8kHz sample rate)

void setup() {
  Serial.begin(115200);
  pinMode(irPin, OUTPUT);
}

void loop() {
  for (size_t i = 0; i < audio_len; i++) {
    byte sample = audio[i];
    sendPAM(sample);
  }
  delay(500);  // delay between replays
}

void sendPAM(byte data) {
  // Map the 8-bit data (0-255) to duty cycle between min and max
  unsigned int dutyCycle = map(data, 0, 255, minDutyCycle, maxDutyCycle);
  
  // Calculate on/off times for the 38kHz carrier based on duty cycle
  unsigned long periodMicros = 1000000 / carrierFreq;  // Period in microseconds
  unsigned long onTime = (periodMicros * dutyCycle) / 100;
  unsigned long offTime = periodMicros - onTime;
  
  // Generate carrier with appropriate duty cycle for the sample duration
  unsigned long startTime = micros();
  unsigned long endTime = startTime + sampleDelay;
  
  while (micros() < endTime) {
    digitalWrite(irPin, HIGH);
    delayMicroseconds(onTime);
    digitalWrite(irPin, LOW);
    delayMicroseconds(offTime);
  }
}